import std.bigint,std.stdio;
import std.conv: to;
BigInt invert(BigInt e, BigInt f)
{
    BigInt a,b,t1,t2=1,t,q;
    a = f;
    b = e;

    while (b!=0) {
        t = a;
        a = b;
        q = t/b;
        b= t%b;
        t = q*t2;
        t = t1-t;
        t1 = t2;
        t2 = t;
        //writeln(a,b,t1,t2,t,q);

    }

    if (t1 <0) {
        t1 += f;
    }
    return t1;
}

